      "Матриця:\n",
      "[[ 2  5  7]\n",
      " [ 6  3  4]\n",
      " [ 5 -2 -3]]\n",
      "\n",
      "Обернена матриця:\n",
      "[[  1.  -1.   1.]\n",
      " [-38.  41. -34.]\n",
      " [ 27. -29.  24.]]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "# Вхідна матриця\n",
    "matrix = np.array([[2, 5, 7],\n",
    "                   [6, 3, 4],\n",
    "                   [5, -2, -3]])\n",
    "# Знаходження оберненої матриці\n",
    "inverse_matrix = np.linalg.inv(matrix)\n",
    "print(\"Матриця:\")\n",
    "print(matrix)\n",
    "print(\"\\nОбернена матриця:\")\n",
    "print(inverse_matrix)\n"
   ]
