   "source": [
    "import numpy as np\n",
    "\n",
    "# Вхідна матриця\n",
    "matrix = np.array([[-1, 2],\n",
    "                   [0, 1]])\n",
    "\n",
    "# Піднесення матриці до квадрату\n",
    "result = np.matmul(matrix, matrix)\n",
    "\n",
    "print(\"Початна матриця:\")\n",
    "print(matrix)\n",
    "\n",
    "print(\"\\nМатриця, піднята до квадрату:\")\n",
    "print(result)\n"
   ]
